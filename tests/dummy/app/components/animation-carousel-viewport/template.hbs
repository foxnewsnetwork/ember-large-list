{{#let (compute this.animeAFF) as |aff|}}
  {{#if (compute this.notEq @animatedValue this.delayedAnimatedValue)}}
    {{force (compute aff.start)}}
  {{/if}}
  {{on-destroy aff.finish}}
  {{#animation-carousel-viewport/-inner
    aff.finish
    animeDirection=this.animeDirection
    isAnimating=this.isAnimating
  }}
    {{yield this.delayedAnimatedValue this.isAnimating}}
  {{/animation-carousel-viewport/-inner}}
{{/let}}