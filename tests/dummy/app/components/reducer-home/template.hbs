{{#model-aff this.getGames as |state|}}
  {{#if state.status.isSuccess}}
    {{reducer-home/reducer-page model=state.model}}
  {{else if state.status.isBusy}}
    Now Loading...
  {{else if state.status.isIdle}}
    Model has yet to run!
  {{else}}
    Encountered Error (uh-oh)
  {{/if}}
{{/model-aff}}