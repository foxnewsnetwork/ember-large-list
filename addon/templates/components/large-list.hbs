{{!-- BEGIN-SNIPPET addon|templates|components|large-list --}}
{{#let
  (compute
  this.UTILS.add
  @startIndex
  @perPage)
as |endIndex|
}}
  {{#let
    (compute
    this.UTILS.slice
    @startIndex
    endIndex
    @items)
  as |subSectionItems|
  }}

    {{#each (compute this.UTILS.range 0 @perPage) as |index|}}
      {{yield
        (get subSectionItems index)
        (compute this.UTILS.add @startIndex index)
      }}
    {{/each}}

  {{/let}}
{{/let}}
{{!-- END-SNIPPET --}}