{{#let
  (compute
  this.UTILS.skipTake
  @startIndex
  @perPage
  @items)
as |subSectionIter|
}}

  {{#each (compute this.UTILS.range 0 @perPage) as |index|}}
    {{yield
      (compute this.UTILS.get subSectionIter index)
      (compute this.UTILS.add @startIndex index)
      index
    }}
  {{/each}}

{{/let}}
{{!-- END-SNIPPET --}}