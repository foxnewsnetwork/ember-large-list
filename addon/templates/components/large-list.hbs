{{!-- BEGIN-SNIPPET addon|templates|components|large-list --}}
{{#let
  (compute
    this.UTILS.skip
    @startIndex
    @items
  )
as |offsetItems|
}}
  {{#let
    (compute
      this.utils.take
      @perPage
      offsetItems
    )
  as |slicedItems|
  }}

    {{#each (compute this.UTILS.range 0 @perPage) as |index|}}
      {{yield
        (compute this.UTILS.next slicedItems)
        (compute this.UTILS.add @startIndex index)
        index
      }}
    {{/each}}
  {{/let}}
{{/let}}
{{!-- END-SNIPPET --}}